{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
      "organizationId": {
          "type": "number"
      },
      "email": {
          "type": "string",
          "minLength": 4
      },
      "username": {
          "type": "string"
      },
      "department": {
          "type": "string"
      },
      "role": {
          "type": "string"
      },
      "assignees": {
          "type": "array"
      },
      "defaultRestaurant": {
          "type": "number"
      }
  },
  "required": ["organizationId", "email"],
  "additionalProperties": false
}
